{
  "name": "slumpa",
  "version": "0.3.0",
  "description": "A small randomizing library. Uses a seed to generate random ints, floats, arrays and more.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "build": "tsc -p tsconfig-build.json",
    "dev": "nodemon",
    "lint": "eslint ./src/**/*.ts",
    "test": "TS_NODE_PROJECT=./tsconfig.json mocha -r ts-node/register/transpile-only --extension ts './test/**/*.test.ts'"
  },
  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "chai": "^4.3.4",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^9.1.1",
    "nodemon": "^2.0.12",
    "prettier": "^2.4.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teolag/slumpa.git"
  },
  "keywords": [
    "random",
    "slump",
    "seed",
    "putback"
  ],
  "author": "Teodor Lagerqvist",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/teolag/slumpa/issues"
  },
  "homepage": "https://github.com/teolag/slumpa#readme",
  "files": [
    "/dist"
  ]
}
